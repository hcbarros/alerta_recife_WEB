<?xml version="1.0"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
              version="2.0">
   
  <navigation-rule>
        <from-view-id>/indexLogin.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>encontrado</from-outcome>
            <to-view-id>/usuario/principal.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>invalido</from-outcome>
            <to-view-id>/indexLogin.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>admin</from-outcome>
            <to-view-id>/adm/principal.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/indexCadastro.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>encontrado</from-outcome>
            <to-view-id>/indexLogin.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>invalido</from-outcome>
            <to-view-id>/indexCadastro.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/usuario/*</from-view-id>
        <navigation-case>
            <from-outcome>servico</from-outcome>
            <to-view-id>/usuario/servico.xhtml</to-view-id>
        </navigation-case> 
        <navigation-case>
            <from-outcome>retornoservico</from-outcome>
            <to-view-id>/usuario/principal.xhtml</to-view-id>
        </navigation-case>  
        <navigation-case>
            <from-outcome>alteradados</from-outcome>
            <to-view-id>/usuario/alterarDados.xhtml</to-view-id>
        </navigation-case>      
    </navigation-rule>   
    <navigation-rule>
        <from-view-id>/adm/*</from-view-id>
        <navigation-case>
            <from-outcome>servico</from-outcome>
            <to-view-id>/adm/servico.xhtml</to-view-id>
        </navigation-case> 
        <navigation-case>
            <from-outcome>retornoservico</from-outcome>
            <to-view-id>/adm/principal.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>usuario</from-outcome>
            <to-view-id>/adm/usuarios.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>alterausuario</from-outcome>
            <to-view-id>/adm/alteraUsuario.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>alteradados</from-outcome>
            <to-view-id>/adm/alterarDados.xhtml</to-view-id>
        </navigation-case> 
        <navigation-case>
            <from-outcome>telefone</from-outcome>
            <to-view-id>/adm/principal.xhtml</to-view-id>
        </navigation-case>         
        <navigation-case>
            <from-outcome>cadastro</from-outcome>
            <to-view-id>/adm/principal.xhtml</to-view-id>
        </navigation-case>  
    </navigation-rule>   
    <navigation-rule>
        <from-view-id>/usuario/telefone.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>telefone</from-outcome>
            <to-view-id>/usuario/principal.xhtml</to-view-id>
        </navigation-case>            
    </navigation-rule>    
    
</faces-config>